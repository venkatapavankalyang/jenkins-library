pipeline{
    agent any

    tools {
        jdk 'java'
    }

    stages{
        stage('checkout'){
            steps{
                checkout([$class: 'GitSCM', branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[credentialsId: 'github access', url: 'https://github.com/venkatapavankalyang/maven-project.git']]])
            }
        }
        stage('mvn build'){
            steps{
                sh 'mvn package'
            }
        }
    }
}